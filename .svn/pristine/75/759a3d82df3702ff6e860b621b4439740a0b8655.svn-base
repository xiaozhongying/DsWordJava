<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="testwork.model">
	<class name="Demo" table="demo" dynamic-insert="true" dynamic-update="true">
		<id name="id" column="ID" type="long">
			<generator class="assigned" />
		</id>
		<property name="title" column="TITLE" type="string" unique="true" length="20" not-null="true" />
		<property name="content" column="CONTENT" type="string" length="50" not-null="true" />
		<property name="foundtime" column="FOUNDTIME" type="string" length="50" not-null="true" />
	</class>
</hibernate-mapping>